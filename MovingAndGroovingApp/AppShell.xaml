<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MovingAndGroovingApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MovingAndGroovingApp"
    xmlns:views="clr-namespace:MovingAndGroovingApp.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="MovingAndGroovingApp">

    <ShellContent
     Title="Login"
     ContentTemplate="{DataTemplate views:LoginPage}"
     Route="LoginPage" />
    <ShellContent
        Title="Workout Log"
        ContentTemplate="{DataTemplate views:WorkoutLogPage}"
        Route="WorkoutLogPage" />

</Shell>
